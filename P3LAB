# Initialization
is_leap_year = False
input_year = int(input())

# Check if year is a leap year
if (input_year % 4 == 0):
    if (input_year % 100 == 0):
        if (input_year % 400 == 0):
            is_leap_year = True
    else:
        is_leap_year = True

# Output the result
if is_leap_year:
    print(f"{input_year} - leap year")
else:
    print(f"{input_year} - not a leap year")
